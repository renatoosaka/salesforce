<!DOCTYPE html>
<html lang="en">
  <head>
    <title></title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  </head>
  <body>
    <isset scope="page" name="products" value="${pdict.products}" />

    <isloop items="${products}" var="product" status="loopstatus">
